<style type="text/css">
   .collect{
      cursor:pointer;
   }
</style>
<div class="breadcrumbs">
    <div class="container">
        <a href="__MODULE__/index/index">首页</a>   
    </div>
</div>

<div class="content">
    <div class="container">
        <div class="order-list-box clearfix">
            <ul class="order-list">             
                <li><a href="" rel="nofollow">新品</a></li>
            </ul>                              
        </div>
                <div class="goods-list-box">
            <div class="goods-list clearfix">

<!-------------------------------------------- 商品列表 ------------------------------------>
            <foreach name="res2" item="attgoods">
                <div class="goods-item">
                    <div class="figure figure-img">
                        <a href="__MODULE__/Goodsdetail/attdetail/gid/<{$attgoods.gid}>"><img src="__PUBLIC__/Uploads/<{$attgoods.pic}>" alt="" height="200" width="200"></a>
                    </div>           
                    <h2 class="title">
                        <a href="__MODULE__/Goodsdetail/attdetail/gid/<{$attgoods.gid}>"><{$attgoods.gname}></a>
                    </h2>
                    <p class="price"><{$attgoods.price}> <del>49元</del></p>
                    <div class="thumbs">
                        <ul class="thumb-list clearfix">
                            <!-- <li><img src="__PUBLIC__/Uploads/<{$attgoods.pic}>" alt="" height="34" width="34"></li>  -->
                            <li><img src="__PUBLIC__/Uploads/jfdh.jpg" class="jpg" title="<{$attgoods.gid}>" alt="<{$attgoods.price}>" height="34" width="34"></li>      
                        </ul>
                        
                    </div>

                    <in name="attgoods.gid" value="$collection">
                        <div  class="collect" gid="<{$attgoods.gid}>">已收藏</div>         
                    <else />
                        <div  class="collect" gid="<{$attgoods.gid}>">加入收藏</div>
                    </in>

                </div>
            </foreach>
<!--------------------------------------- 商品列表结束---------------------------------->
             </div>
             <script>
                $(function(){
                    $('.jpg').click(function(){
                        var jpg=$(this).attr('alt');
                        // var gid=$(this).attr('title');
                        $.ajax({
                           url:'__MODULE__/Product/integral',
                           type:'post',
                           data:{'jpg':jpg},
                           dataType:'json',
                          // async:true;
                           success:function(data){
                           if(data){
                                alert('您的商品兑换成功,我们会在7个工作日内邮寄到你的常用地址')
                           // location.reload();
                            }else{  
                            alert('您的积分不足以兑换此商品');         
                            }   
                        }                
                    })
                })

            })
                


             </script>
            
